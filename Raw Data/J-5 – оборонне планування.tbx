            <conceptEntry id="331366">
                <descrip type="translatable">yes</descrip>
                <descrip type="subjectField">500 – оборонне планування</descrip>
                <langSec xml:lang="en">
                    <termSec>
                        <term id="690352">nation</term>
                        <descrip type="context">A large number of people who share the same history, ancestors, culture. </descrip>
                        <xref type="externalCrossReference" target="https://dictionary.cambridge.org/uk/dictionary/english-ukrainian/nation"></xref>
                    </termSec>
                </langSec>
                <langSec xml:lang="uk">
                    <termSec>
                        <term id="690354">країна</term>
                        <descrip type="context">Територія, що становить єдність із погляду історії, природних умов, населення тощо.</descrip>
                        <xref type="externalCrossReference" target="https://slovnyk.ua/index.php?swrd=країна"></xref>
